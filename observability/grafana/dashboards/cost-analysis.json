{
  "uid": "openclaw-cost-analysis",
  "title": "Cost Analysis",
  "description": "USD cost breakdown by model, provider, and agent",
  "tags": ["openclaw", "cost", "billing"],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "10s",
  "schemaVersion": 39,
  "version": 1,
  "templating": {
    "list": [
      {
        "name": "agent",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(openclaw_session_cost_usd, agent)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      },
      {
        "name": "model",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(openclaw_session_cost_usd{agent=~\"$agent\"}, model)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      },
      {
        "name": "provider",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\"}, provider)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Cost (1h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[1h]))",
          "legendFormat": "Total Cost",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 25 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Input Tokens (1h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(openclaw_session_tokens_total{agent=~\"$agent\", model=~\"$model\", type=\"input\"}[1h]))",
          "legendFormat": "Input Tokens",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "Output Tokens (1h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(openclaw_session_tokens_total{agent=~\"$agent\", model=~\"$model\", type=\"output\"}[1h]))",
          "legendFormat": "Output Tokens",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "purple", "value": null }]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "Cost by Model",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (model) (increase(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[1h]))",
          "legendFormat": "{{ model }}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "pieType": "donut",
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 5,
      "title": "Cost by Provider",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (provider) (increase(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[1h]))",
          "legendFormat": "{{ provider }}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "pieType": "donut",
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "title": "Cost by Agent",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (agent) (increase(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[1h]))",
          "legendFormat": "{{ agent }}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "pieType": "donut",
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 7,
      "title": "Cost Over Time (by Model)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (model) (rate(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[5m])) * 3600",
          "legendFormat": "{{ model }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisLabel": "$/hour",
            "drawStyle": "bars",
            "fillOpacity": 60,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "never",
            "stacking": { "group": "A", "mode": "normal" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 8,
      "title": "Cost Over Time (by Provider)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (provider) (rate(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[5m])) * 3600",
          "legendFormat": "{{ provider }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisLabel": "$/hour",
            "drawStyle": "bars",
            "fillOpacity": 60,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "never",
            "stacking": { "group": "A", "mode": "normal" }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 9,
      "title": "Token Cost Efficiency (Cost per 1K Tokens)",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (model, provider) (increase(openclaw_session_cost_usd{agent=~\"$agent\", model=~\"$model\", provider=~\"$provider\"}[1h])) / (sum by (model, provider) (increase(openclaw_session_tokens_total{agent=~\"$agent\", model=~\"$model\"}[1h])) / 1000)",
          "legendFormat": "{{ model }} ({{ provider }})",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.1 }
            ]
          },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "color-text" },
            "inspect": false
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Value", "desc": true }],
        "footer": { "enablePagination": false }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "model": "Model",
              "provider": "Provider",
              "Value": "$/1K Tokens"
            }
          }
        }
      ]
    }
  ]
}
